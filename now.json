{
  "version": 2,
  "name": "prism",
  "alias": "prismreader.app",
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@now/node"
    },
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/node/(.*)",
      "dest": "/api/node"
    },
    {
      "src": "/api/radiks/(.*)",
      "dest": "/api/radiks"
    },
    {
      "src": "/api/invoice(.*)",
      "dest": "/api/invoice"
    },
    {
      "src": "/api/proofs(.*)",
      "dest": "/api/proofs"
    },
    {
      "handle": "filesystem"
    }
  ]
}
